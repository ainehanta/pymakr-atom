{
  "name": "Pymakr",
  "main": "./lib/main.js",
  "version": "1.0.0",
  "description": "Adds a REPL console to Atom that connects to your Pycom board. It can run code on the board or synchronize your project files to it.",
  "keywords": [
    "Pycom",
    "Pymakr",
    "MicroPython",
    "WiPy",
    "Wipy2",
    "LoPy",
    "SiPy",
    "FiPy"
  ],
  "repository": "https://github.com/pycom/pymakr-atom",
  "license": "GPL-3.0",
  "engines": {
    "atom": ">=1.0.0 <2.0.0",
    "node": ">=6.3.0 <=7.0.0"
  },
  "dependencies": {
    "telnet-client": "^0.13.0",
    "xterm": "^2.3.2",
    "element-resize-detector": "1.1.11",
    "font-awesome": "^4.7.0"
  },
  "scripts": {
    "install": "node install.js"
  }
}
